<h2>Rediger firma: <%= @business.name %></h2>

<%= form_for @changeset, Routes.business_path(@conn, :update, @business), fn f -> %>
  <div class="form-group">
    <%= label f, :dinero_api_key, "Dinero API nÃ¸gle" %>
    <%= text_input f, :dinero_api_key, class: "form-control" %>
    <%= error_tag f, :dinero_api_key %>
  </div>
  <div class="form-group">
    <%= label f, :dinero_id, "Dinero ID" %>
    <%= text_input f, :dinero_id, class: "form-control" %>
    <%= error_tag f, :dinero_id %>
  </div>
  <div>
    <%= submit "Gem" %>
  </div>
<% end %>